                        }
                    }
                },
                "parameters": [
                    {
                        "name": "Transfer",
                        "in": "body",
                        "description": "The transfer negotiation document",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/Transfer"
                        }
                    }
                ]
            }
        },
